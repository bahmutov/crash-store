{
  "name": "crash-store",
  "version": "1.0.0",
  "description": "Receives and stores JavaScript exceptions",
  "main": "index.js",
  "scripts": {
    "test": "eslint index.js src/**/*.js",
    "issues": "git-issues",
    "commit": "git-issues && commit-wizard",
    "start": "node index.js",
    "watch": "nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahmutov/crash-store.git"
  },
  "keywords": [
    "crash",
    "exception",
    "store",
    "db"
  ],
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahmutov/crash-store/issues"
  },
  "homepage": "https://github.com/bahmutov/crash-store#readme",
  "dependencies": {
    "bluebird": "3.0.5",
    "check-more-types": "2.1.2",
    "crash-reporter-middleware": "1.2.7",
    "crash-store-db": "1.1.1",
    "error-receiver": "1.4.0",
    "express": "4.13.3",
    "lazy-ass": "1.0.0",
    "nconf": "0.8.2"
  },
  "config": {
    "pre-git": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": "npm test",
      "pre-push": [],
      "post-commit": [],
      "post-merge": []
    }
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "devDependencies": {
    "eslint": "1.9.0",
    "git-issues": "1.1.0",
    "nodemon": "1.8.1",
    "pre-git": "1.2.11"
  }
}
